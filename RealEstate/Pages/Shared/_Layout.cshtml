<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RealEstate</title>
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
@*     <link rel="stylesheet" href="~/lib/bootstrap/dist/materialCss/mdb.min.css" />
 *@@*     <link rel="stylesheet" href="~/lib/bootstrap/dist/materialCss/mdb.dark.min.css" /> *@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="icon" type="image/x-icon" href="/images/icon/logo.svg">
    <link rel="stylesheet" href="~/icons/fontawesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/RealEstate.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/js/animate/animate.css" />
    <script src="~/js/sweet_alert/swap.js"></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow fixed-top m-0">
                <a class="navbar-brand" asp-page="/Index">
                    <img src="/images/icon/logo.svg" class="brand-animation position-relative ms-2"/>
                </a>
                <a class="navbar-brand fw-bold" asp-area="" asp-page="/Index">RealEstate</a>
                <button class="navbar-toggler mx-2" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex px-4 justify-content-between">
                    <ul class="navbar-nav flex-grow-1 my-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Privacy</a>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
        </nav>
    </header>
    <div class="row justify-content-center pt-5 mt-5" >
        <partial name="_MessagePartial" />
        <main role="main" class="p-0" >
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; <span>@DateTime.Now.Year.ToString()</span> -
            <i class="fa fa-linkedin me-2"></i><a href="https://www.linkedin.com/in/ashkan-motahari/" class="fs-7 text-muted text-decoration-none">Ashkan Amjad</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
@*     <script src="~/lib/bootstrap/dist/materialJs/mdb.es.min.js"></script>
 *@ <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>