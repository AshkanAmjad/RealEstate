@if (TempData["MessageType"] != null)
{
    string errorType = (string)TempData["MessageType"];
    if (errorType == "error")
    {
        <script>
            swal.fire({
                title: 'Error',
                icon: 'error',
                showConfirmButton: false,
                timer: 4000,
                position: 'top-end',
                showClass: {
                    popup: 'animate__animated animate__fadeInDown'
                },
                hideClass: {
                    popup: 'animate__animated animate__fadeOutDown'

                }

            })
        </script>
    }
    else if (errorType == "success")
    {
        <script>
            swal.fire({
                title: 'Successfuly operation.',
                icon: 'success',
                showConfirmButton: false,
                timer: 4000,
                showClass: {
                    popup: 'animate__animated animate__fadeInDown'
                },
                hideClass: {
                    popup: 'animate__animated animate__fadeOutDown'

                }
            })
        </script>
    }
    else if (errorType == "availableError")
    {
        <script>
            swal.fire({
                title: 'Error',
                icon: 'error',
                text: 'There is no item available.',
                customClass: {
                    confirmButton: 'btn btn-danger px-4'
                },
                showClass: {
                    popup: 'animate__animated animate__fadeInDown'
                },
                hideClass: {
                    popup: 'animate__animated animate__fadeOutDown'

                },
                buttonsStyling: false
            })
        </Script>
    }
    else if (errorType == "similarityError")
    {
        <script>
            swal.fire({
                title: 'Error',
                icon: 'error',
                text: 'This data is recorded for another user',
                customClass: {
                    confirmButton: 'btn btn-danger px-4'
                },
                showClass: {
                    popup: 'animate__animated animate__fadeInDown'
                },
                hideClass: {
                    popup: 'animate__animated animate__fadeOutDown'

                },
                buttonsStyling: false
            })
        </Script>
    }
    else if (errorType == "searchError") {
        <script>
            swal.fire({
                title: 'Error',
                icon: 'error',
                text: 'Completion of two content inputs and filters is required.',
                customClass: {
                    confirmButton: 'btn btn-danger px-4'
                },
                showClass: {
                    popup: 'animate__animated animate__fadeInDown'
                },
                hideClass: {
                    popup: 'animate__animated animate__fadeOutDown'

                },
                buttonsStyling: false

            })
        </script>
    }
}